{
    "name": "dsr/errorpages",
    "type": "phpbb-extension",
    "description": "Error Pages displays Apache errors in your forums style.",
    "version": "4.0.0",
    "keywords": [
        "phpbb",
        "extension",
        "error",
        "error pages"
    ],
    "license": "GPL-2.0",
    "authors": [
        {
            "name": "John Peskens",
            "homepage": "http://forumhulp.com",
            "email": "info@forumhulp.com",
            "role": "Developer"
        }, {
            "name": "DSR!",
            "email": "xchwarze@gmail.com",
            "homepage": "https://github.com/xchwarze",
            "role": "owner"
        }
    ],
    "require": {
        "php": ">=5.6.0"
    },
    "extra": {
        "display-name": "Error pages",
        "soft-require": {
            "phpbb/phpbb": ">=3.2.0"
        },
        "version-check": {
            "host": "raw.githubusercontent.com",
            "directory": "/xchwarze/errorpages-phpbb/master",
            "filename": "last_ext_version.json",
            "ssl": true
        }
    }
}